{
  "name": "Passionfruit",
  "version": "0.0.3",
  "description": "Inject to iOS App",
  "main": "app.js",
  "engines": {
    "node": ">= 7.10.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "es2015",
              {
                "loose": true
              }
            ]
          ],
          "plugins": [
            "transform-runtime"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon .",
    "start": "cross-env NODE_ENV=production node .",
    "build": "npm run compile && cd gui && (yarn run build || npm run build)",
    "compile": "node compile.js compile",
    "watch": "node compile.js watch",
    "postinstall": "cd gui && (yarn || npm install)"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/chaitin/passionfruit.git"
  },
  "keywords": [
    "iOS",
    "frida"
  ],
  "author": "CodeColorist",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chaitin/passionfruit/issues"
  },
  "homepage": "https://github.com/chaitin/passionfruit#readme",
  "dependencies": {
    "base64-js": "^1.2.1",
    "eslint": "^4.8.0",
    "frida": "^10.6.3",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compress": "^2.0.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.0.1",
    "koa-router": "^7.2.1",
    "koa-send": "^4.1.1",
    "nodemon": "^1.12.1",
    "plist": "^2.1.0",
    "promise.prototype.finally": "^3.0.0",
    "socket.io": "^2.0.3",
    "socket.io-stream": "^0.9.1",
    "sqlite3": "^3.1.9",
    "frida-compile": "^5.1.0"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "babelify": "^7.3.0",
    "cross-env": "^5.0.5",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.7.0",
    "macho": "^1.2.1"
  }
}
